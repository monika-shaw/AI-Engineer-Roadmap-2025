   "source": [
    "import sympy as sp\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "f = 3*x**3 - 5*x**2 + 7*x - 2\n",
    "\n",
    "f_prime = sp.diff(f, x)\n",
    "f_prime\n"
