   "source": [
    "Q1. Compute cosine similarity between two vectors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "55545e9e-3b45-48ba-b43d-38c67a49b368",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Cosine Similarity: 0.7937312641344627\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "# Two feature vectors\n",
    "a = np.array([2, 3, 4])\n",
    "b = np.array([1, 0, 6])\n",
    "\n",
    "# Cosine similarity\n",
    "cos_sim = np.dot(a, b) / (np.linalg.norm(a) * np.linalg.norm(b))\n",
    "print(\"Cosine Similarity:\", cos_sim)\n"
   ]
  },
